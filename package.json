{
  "name": "jspinch",
  "version": "0.0.2",
  "author": "Clay Smith @smithclay",
  "description": "a tool for tracking code quality with jshint and jslint",
  "bin": {
    "jspinch": "./bin/jspinch"
  },
  "scripts": {
    "start": "node ./bin/jspinch",
    "test": "vows --spec --isolate"
  },
  "main": "./lib/cli",
  "repository": {
    "type": "git",
    "url": "https://github.com/smithclay/jspinch.git"
  },
  "keywords": [
    "jshint",
	"jslint",
	"code quality",
    "static analysis"
	],
  "dependencies" : {
    "jshint"   :  "*",
	"jslint" : "*",
	"colors" : "*",
	"glob": "*",
    "minimatch" : "*",
	"cli-table": "*",
	"commander" : ">=0.5.0"
  },
  "devDependencies": {
    "vows"    :  "0.5.x"
  },
  "license": "MIT",
  "engine": {
    "node": ">=0.6"
  }
}